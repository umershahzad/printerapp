        }
    }
    *Option: DocumentTop
    {
        *rcNameID: =PERIPHERAL_UNIT__DOC_TOP
        *switch: PeripheralUnitType
        {
            *case: CashDrawer
            {
                *Command: CmdSelect
                {
                    *Order: JOB_SETUP.10
                    *Cmd: "<07>"
                }
            }
            *case: Buzzer
            {
                *switch: Buzzer1RepeatPrint
                {
                    *case: 1times
                    {
                        *Command: CmdSelect
                        {
                            *Order: JOB_SETUP.10
                            *Cmd: "<1B><1D><19><12>1<01><00>"
                        }
                    }
                    *case: 2times
                    {
                        *Command: CmdSelect
                        {
                            *Order: JOB_SETUP.10
                            *Cmd: "<1B><1D><19><12>1<02><00>"
                        }
                    }
                    *case: 3times
                    {
                        *Command: CmdSelect
                        {
                            *Order: JOB_SETUP.10
                            *Cmd: "<1B><1D><19><12>1<03><00>"
                        }
                    }
                    *case: 5times
                    {
                        *Command: CmdSelect
                        {
                            *Order: JOB_SETUP.10
                            *Cmd: "<1B><1D><19><12>1<05><00>"
                        }
                    }
