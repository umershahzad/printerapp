<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<configuration>
 <CommandEmulator>
   <setting name="Source Emulator Plugin Path" type="string">StarLineModeSE.dll</setting>
   <setting name="Target Converter Plugin Path" type="string">StarTSPTC.dll</setting>
 </CommandEmulator>
 <StarLineModeSE>
   <Defaults>
     <setting name="Max Printable Width" type="string">72mm</setting>
     <setting name="Font" type="string">FontA</setting>
   </Defaults>
 </StarLineModeSE>
 <StarTSPTC>
   <setting name="Model" type="string">TSP100</setting>
   <setting name="Paper Width" type="string">76mm</setting>
 </StarTSPTC>
 <CmdSubPP>
    <setting name="End Of Page Line Feeds" type="uint32">3</setting>
    <Key0>
      <setting name="Key Type" type="string">Cut</setting>
      <setting name="Search" type="string">\1Bd0</setting>
      <setting name="Substitute" type="string">\1Bd0\1B\1Fp3\1B\1Fp0</setting>
    </Key0>
